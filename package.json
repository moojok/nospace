{
  "name": "nospace",
  "version": "0.0.0",
  "private": true,
  "author": "Little Human <hey@moojok.online>",
  "workspaces": [
    "clients/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=@nospace/web",
    "dev": "turbo run dev --filter=@nospace/web",
    "lint": "turbo run lint --parallel",
    "test": "turbo run test --parallel",
    "format:client": "turbo run format --filter=@nospace/web",
    "format:core": "./trunk.sh fmt -a",
    "fix": "manypkg fix"
  },
  "dependencies": {
    "@manypkg/cli": "^0.21.0",
    "turbo": "^1.10.13"
  },
  "packageManager": "yarn@3.6.3"
}
